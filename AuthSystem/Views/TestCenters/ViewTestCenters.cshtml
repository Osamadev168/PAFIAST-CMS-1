@model List<TestCenters>;

@{
    ViewData["Title"] = "Test Centers";
}

<h1>Test Centers</h1>
@if (Model.Count == 0)
{
    <h1>No Test centers created Yet!</h1>
}
@for (var index = 0; index < Model.Count; index++)
{
    <div>
        <h3><strong>@(index + 1): </strong>@Model[index].TestCenterName</h3>
    </div>
}


